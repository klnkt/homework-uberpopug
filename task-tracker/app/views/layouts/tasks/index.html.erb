<p id="notice"><%= notice %></p>

<h1>Tasks</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>title</th>
      <th>description</th>
      <th>assignee</th>
      <th>cost</th>
      <th>reward</th>
      <th>status</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.id %></td>
        <td><%= account.title %></td>
        <td><%= account.description %></td>
        <td><%= account.assignee.full_name %></td>
        <td><%= account.cost %></td>
        <td><%= account.reward %></td>
        <td><%= account.status %></td>
        <td><%= link_to 'Complete', task_complete_path(task) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

